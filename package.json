{
  "name": "ruport-template",
  "version": "1.0.0",
  "description": "simple template for react projects",
  "scripts": {
    "start": "NODE_ENV=development ./node_modules/.bin/parcel public/index.html --no-cache --no-source-maps --lazy --port 3000 --dist-dir build --public-url /",
    "build": "NODE_ENV=production ./node_modules/.bin/parcel build public/index.html --dist-dir build --no-source-maps --no-cache --public-url /app",
    "eslint": "eslint 'src/**/*{.js,.jsx}'",
    "eslint:fix": "eslint 'src/**/*{.css,.scss}' --fix",
    "stylelint": "stylelint 'src/**/*{.css,.scss}'",
    "stylelint:fix": "stylelint 'src/**/*{.css,.scss}' --fix",
    "test": "jest",
    "test:ci": "jest --ci --coverage",
    "test:update": "npm run test -- --u",
    "test:watch": "npm run test -- --watch"
  },
  "author": "ruport",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/plugin-transform-modules-commonjs": "7.17.7",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.17.12",
    "@parcel/babel-preset-env": "^2.9.3",
    "@parcel/resolver-glob": "2.7.0",
    "@parcel/transformer-inline-string": "2.7.0",
    "@parcel/transformer-postcss": "2.7.0",
    "@parcel/transformer-sass": "^2.7.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^12.1.4",
    "@testing-library/react-hooks": "^5.1.2",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.3",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.5",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-test-renderer": "^18.0.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "babel-plugin-module-resolver": "4.1.0",
    "classnames": "^2.3.1",
    "eslint": "8.11.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-testing-library": "^5.11.0",
    "jest-environment-jsdom": "^29.6.1",
    "nyc": "^15.1.0",
    "parcel": "^2.7.0",
    "parcel-plugin-nuke-dist": "1.0.1",
    "prettier": "2.6.0",
    "prettier-eslint": "13.0.0",
    "process": "^0.11.10",
    "react-test-renderer": "^17.0.2",
    "recoil": "^0.7.5",
    "stylelint": "14.6.0",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-rational-order": "0.1.2",
    "stylelint-config-standard": "25.0.0",
    "stylelint-config-standard-scss": "3.0.0",
    "stylelint-order": "5.0.0",
    "stylelint-prettier": "2.0.0",
    "stylelint-scss": "4.2.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "nanoid": "^4.0.0",
    "object-to-formdata": "^4.4.2",
    "query-string": "^7.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.34.0",
    "react-router-dom": "^6.3.0"
  }
}
